		<script>
			$(function() {
				var isLoggedIn = ('<%=isLoggedIn%>' === 'true');
				if( isLoggedIn ) {
					$('.showLogout').show();
					$('.showLogin').hide();
				} else {
					$('.showLogout').hide();
					$('.showLogin').show();
				}
			});
			$(function() {
				var verknExtended = (verknExtended = ('<%=verknExtended%>' === 'true')) ? $('.showverknExtended').show() : $('.showverknExtended').hide();
				var altersvorsorge = (altersvorsorge = ('<%=altersvorsorge%>' === 'true')) ? $('.showaltersvorsorge').show() : $('.showaltersvorsorge').hide();
				var sozialakademie = (sozialakademie = ('<%=sozialakademie%>' === 'true')) ? $('.showsozialakademie').show() : $('.showsozialakademie').hide();
				var knappenbar = (knappenbar = ('<%=knappenbar%>' === 'true')) ? $('.showknappenbar').show() : $('.showknappenbar').hide();
				var gemuesefond = (gemuesefond = ('<%=gemuesefond%>' === 'true')) ? $('.showgemuesefond').show() : $('.showgemuesefond').hide();
				var gluecklichtage = (gluecklichtage = ('<%=gluecklichtage%>' === 'true')) ? $('.showgluecklichtage').show() : $('.showgluecklichtage').hide();
				var paybackpele = (paybackpele = ('<%=paybackpele%>' === 'true')) ? $('.showpaybackpele').show() : $('.showpaybackpele').hide();
				var walzer = (walzer = ('<%=walzer%>' === 'true')) ? $('.showwalzer').show() : $('.showwalzer').hide();
				var diskofox = (diskofox = ('<%=diskofox%>' === 'true')) ? $('.showdiskofox').show() : $('.showdiskofox').hide();
				var chachacha = (chachacha = ('<%=chachacha%>' === 'true')) ? $('.showchachacha').show() : $('.showchachacha').hide();
				var wienerwalzer = (wienerwalzer = ('<%=wienerwalzer%>' === 'true')) ? $('.showwienerwalzer').show() : $('.showwienerwalzer').hide();
				var swing = (swing = ('<%=swing%>' === 'true')) ? $('.showswing').show() : $('.showswing').hide();
				var rumba = (rumba = ('<%=rumba%>' === 'true')) ? $('.showrumba').show() : $('.showrumba').hide();
				var foxtrott = (foxtrott = ('<%=foxtrott%>' === 'true')) ? $('.showfoxtrott').show() : $('.showfoxtrott').hide();
				var blues = (blues = ('<%=blues%>' === 'true')) ? $('.showblues').show() : $('.showblues').hide();
			});
		</script>
	</head>
	<body id="profil" class="coops">
		
		<div id="nav-icon">
			<span></span>
			<span></span>
			<span></span>
			<span></span>
		</div>
		
		<nav id="main-nav">
			<div class="container-fluid">
				<div class="row">
					<div class="col-12">
						<div id="header">
							<a href="/">
								<img src="/public/files/images/COO_MENU_Logo.jpg" class="header-logo" title="Logo Coop3000" />
							</a>								
						</div>
						<div class="content">
							<nav>
								<% include ../elements/navigation %>
							</nav>
						</div>						
					</div>
				</div>					
			</div>
		</nav>
		
		<div id="fullpage">
			<section class="section" id="section0">
				<div class="container-fluid">
					<div class="row">
						<div class="col-12">
							<div id="header">
								<a href="/">
									<img src="/public/files/images/COO_03_Registrierung_Logo.png" class="header-logo" title="Logo Coop3000" />
								</a>	
							</div>
							
							<div class="container">
								<div class="row">
									<div class="col-lg-8 col-lg-offset-2">
										<div class="content">
											<h1>
												Profil
											</h1>

											<ul class="subnav">
												<li><a href="/listvotes">ALLE VORSCHLÄGE</a></li>
												<li><a href="/voteactivity">MEINE VORSCHLÄGE</a></li>
												<li><span>Anteile</span></li>
												<li><a href="/follow">Anhängen</a></li>
												<li><a href="/search">SUCHEN</a></li>
												<li><a href="/profil">Profil</a></li>
												<li><a href="/logout">Logout</a></li>
											</ul>

											<h1>
												ZUSAMMEN EINEN CONCERN BAUEN
											</h1>
											<br>
											<p>Du hast bisher mit diesen Anteilen den Concern erweitert:</p>
											
											<span class="divider"><span></span></span>
											
											<h1>
												STUFE 1 / DAS FÜLLHORN<br>
												<span class="subline">
													STUFE 1 / OKTOBER 016
												</span>
											</h1>
											<form id="coopform">
												<fieldset>
													<legend>EINEN BALL MACHEN</legend>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Communities knüpfen
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="verknExtended" id="verknExtended" <%= verknExtended %> disabled>
													    	<label for="verknExtended"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Altersvorsorge (temporär)
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="altersvorsorge" id="altersvorsorge" <%= altersvorsorge %> disabled>
													    	<label for="altersvorsorge"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Sozialakademie
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="sozialakademie" id="sozialakademie" <%= sozialakademie %> disabled>
													    	<label for="sozialakademie"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Knappenbar
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="knappenbar" id="knappenbar" <%= knappenbar %> disabled>
													    	<label for="knappenbar"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Gemüsefonds
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="gemuesefond" id="gemuesefond" <%= gemuesefond %> disabled>
													    	<label for="gemuesefond"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Glückliche Tage
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="gluecklichtage" id="gluecklichtage" <%= gluecklichtage %> disabled>
													    	<label for="gluecklichtage"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
													        Payback-Pele
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="paybackpele" id="paybackpele" <%= paybackpele %> disabled>
													    	<label for="paybackpele"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
												</fieldset>
											
												<span class="divider"><span></span></span>
											
												<fieldset>
													<legend>TÄNZE 3000</legend>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    LANGSAMER WALZWERKWALZER
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="walzer" id="walzer" <%= walzer %> disabled>
													    	<label for="walzer"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    DISKOFOX RHEINHAUSEN
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="diskofox" id="diskofox" <%= diskofox %> disabled>
													    	<label for="diskofox"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    CHACHACHA AUF DER WELTFRAUENKONFERENZ
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="chachacha" id="chachacha" <%= chachacha %> disabled>
													    	<label for="chachacha"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    WIENER GEMEINDEBAU-WALZER
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="wienerwalzer" id="wienerwalzer" <%= wienerwalzer %> disabled>
													    	<label for="wienerwalzer"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    SWING AM FLIESSBAND
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="swing" id="swing" <%= swing %> disabled>
													    	<label for="swing"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    RUMBA SOLIDARIA
													        
													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="rumba" id="rumba" <%= rumba %> disabled>
													    	<label for="rumba"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    FOXTROTT HINTERM LAUTSPRECHERWAGEN

													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="foxtrott" id="foxtrott" <%= foxtrott %> disabled>
													    	<label for="foxtrott"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
													<div class="form-group mycheckboxdiv">
													    <label>
														    BLUES BRITANNIA

													    </label>
													    <div class="squaredTwo">
													    	<input type="checkbox" name="blues" id="blues" <%= blues %> disabled>
													    	<label for="blues"></label>
													    </div>
													    <span class="clearer"></span>
													</div>
													
												</fieldset>
											
												<span class="divider"><span></span></span>
											
												<h1>
													STUFE 2 / DIE SUPPERTHEORIEPARTIES<br>
													<span class="subline">
														STUFE 2 / APRIL-JUNI 017
													</span>
												</h1>
												<% if(events!= undefined && events.length>0){%>
													<fieldset>
														<legend>DAS GELÄNDE SONDIEREN</legend>
														
														
														<% for(var i=0; i<events.length; i++) {%>
															<% if(i===4){break;}%>
															<% var checked = '';%>
															
															<% if(events[i].participants!= undefined && events[i].participants.length>0){
																for(var j = 0; j < events[i].participants.length; j++) {
																	if(events[i].participants[j].id == id) {
																		checked = 'checked';
																	}
																}
															} %>
															
															<div class="form-group mycheckboxdiv">
															    <label>
															        <%= events[i].name %>
															        
															    </label>
															    <div class="squaredTwo">
															    	<input type="checkbox" name="<%= events[i].name %>" id="<%= events[i].name %>" <%= checked %> disabled>
															    	<label for="<%= events[i].name %>"></label>
															    </div>
																	<span class="clearer"></span>
															</div>
				
														<% } %>
													</fieldset>

													<h1>
														STUFE 3 / DER CONCERN WIRD ERÖFFNET<br>
														<span class="subline">
															STUFE 3 / JULI 017
														</span>
													</h1>

													<fieldset>
														<legend>DER CONCERN HANDELT</legend>

														<% if(events!= undefined && events.length>4){%>
															<% for(var i=4; i<events.length; i++) {%>
																
																<% var checked2 = '';%>
																<% if(events[i].participants!= undefined && events[i].participants.length>0){
																	for(var j = 0; j < events[i].participants.length; j++) {
																		if(events[i].participants[j].id == id) {
																			checked2 = 'checked';
																		}
																	}
																} %>
																<div class="form-group mycheckboxdiv">
															    <label>
															        <%= events[i].name %>
															        
															    </label>
															    <div class="squaredTwo">
															    	<input type="checkbox" name="<%= events[i].name %>" id="<%= events[i].name %>" <%= checked2 %> disabled>
															    	<label for="<%= events[i].name %>"></label>
															    </div>
																	<span class="clearer"></span>
															</div>
															<% } %>
														<% }else{ %>
															Es gibt noch keine Concern Handlungen.
														<% } %>
													</fieldset>
												<% }else{%>
													Es gibt noch keine Events
												<% } %>
											
											</form>
										</div>
									</div>
								</div>
							</div>
							<div class="col-lg-8 col-lg-offset-2">
								<div class="content">
								</div>
						</div>
					</div>
				</div>
			</section>
		</div>
	    
	    <script src="/public/js/jquery.easing.min.js"></script>
	    <script src="/public/js/jquery.fullPage.min.js"></script>
	    <script src="/public/js/custom.js"></script>
	</body>
</html>
