		<div id="fullpage">
			<section class="section" id="section0">
				<div class="container-fluid">
					<div class="row">
						<div class="col-12">
							<div id="header">
								<a href="/">
									<img src="/public/files/images/COO_03_Registrierung_Logo.png" class="header-logo" title="Logo Coop3000" />
								</a>	
							</div>
							
							<div class="container">
								<div class="row">
									<div class="col-lg-8 col-lg-offset-2">
										<div class="content">
											<h1>
												Profil
											</h1>
											
											<ul class="subnav">
												<li><a href="/profil">Profil</a></li>
												<li><span>COOPS</span></li>
												<li><a href="/follow">Anhängen</a></li>
												<li><a href="/logout">Logout</a></li>
											</ul>

											<h1>
												ZUSAMMEN EINEN CONCERN BAUEN
											</h1>
											<br>
											Du hast bisher mit diesen COOPS deinen Anteil im Concern erweitert:
											<hr>
											<h1>
												STUFE 1 / DAS FÜLLHORN
											</h1>
											<h2>
												EINEN BALL MACHEN
											</h2>
											<br>
											COMMUNITIES KNÜPFEN
											<input type="checkbox" name="verknExtended" id="verknExtended" <%= verknExtended %> disabled><br>
											ALTERSVORSORGE (TEMPORÄR)
											<input type="checkbox" name="altersvorsorge" id="altersvorsorge" <%= altersvorsorge %> disabled><br>
											SOZIALAKADEMIE
											<input type="checkbox" name="sozialakademie" id="sozialakademie" <%= sozialakademie %> disabled><br>
											KNAPPENBAR
											<input type="checkbox" name="knappenbar" id="knappenbar" <%= knappenbar %> disabled><br>
											GEMÜSEFONDS
											<input type="checkbox" name="gemuesefond" id="gemuesefond" <%= gemuesefond %> disabled><br>
											GLÜCKLICHE TAGE
											<input type="checkbox" name="gluecklichtage" id="gluecklichtage" <%= gluecklichtage %> disabled><br>
											PAYBACK-PELE
											<input type="checkbox" name="paybackpele" id="paybackpele" <%= paybackpele %> disabled><br>
											<h2>
												TÄNZE 3000
											</h2>
											<br>
											LANGSAMER WALZER
											<input type="checkbox" name="walzer" id="walzer" <%= walzer %> disabled><br>
											DISKOFOX
											<input type="checkbox" name="diskofox" id="diskofox" <%= diskofox %> disabled><br>
											CHA CHA CHA
											<input type="checkbox" name="chachacha" id="chachacha" <%= chachacha %> disabled><br>
											WIENER WALZER
											<input type="checkbox" name="wienerwalzer" id="wienerwalzer" <%= wienerwalzer %> disabled><br>
											SWING
											<input type="checkbox" name="swing" id="swing" <%= swing %> disabled><br>
											RUMBA
											<input type="checkbox" name="rumba" id="rumba" <%= rumba %> disabled><br>
											FOXTROTT
											<input type="checkbox" name="foxtrott" id="foxtrott" <%= foxtrott %> disabled><br>
											BLUES
											<input type="checkbox" name="blues" id="blues" <%= blues %> disabled><br>
											<hr>
											<h1>
												STUFE 2 / DIE SUPPERTHEORIEPARTIES
											</h1>
											<% if(events!= undefined && events.length>0){%>
												<h2>
													DAS GELÄNDE SONDIEREN
												</h2>
												<br>
												<% for(var i=0; i<events.length; i++) {%>
													<% if(i===4){break;}%>
													<% var checked = '';%>
													<%= events[i].name %>
													<% if(events[i].participants!= undefined && events[i].participants.length>0){
														for(var j = 0; j < events[i].participants.length; j++) {
															if(events[i].participants[j].id == id) {
																checked = 'checked';
															}
														}
													} %>
													<input type="checkbox" name="<%= events[i].name %>" id="<%= events[i].name %>" <%= checked %> disabled><br>
												<% } %>
												<h2>
													DER CONCERN HANDELT
												</h2>
												<br>
												<% if(events!= undefined && events.length>4){%>
													<% for(var i=4; i<events.length; i++) {%>
														<%= events[i].name %>
														<% var checked2 = '';%>
														<% if(events[i].participants!= undefined && events[i].participants.length>0){
															for(var j = 0; j < events[i].participants.length; j++) {
																if(events[i].participants[j].id == id) {
																	checked2 = 'checked';
																}
															}
														} %>
														<input type="checkbox" name="<%= events[i].name %>" id="<%= events[i].name %>" <%= checked2 %> disabled><br>
													<% } %>
												<% }else{ %>
													Es gibt noch keine Concern Handlungen.
												<% } %>
											<% }else{%>
												Es gibt noch keine Events
											<% } %>
										</div>
									</div>
								</div>
							</div>
							<div class="col-lg-8 col-lg-offset-2">
								<div class="content">
								</div>
						</div>
					</div>
				</div>
			</section>
		</div>
	    
